	local Settings = Window:NewTab("Settings And Credits")
    local SettingsSection = Settings:NewSection("Made By rmg#3285 DM Me For Status Updates")
 
SettingsSection:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.RightShift, function()
	Library:ToggleUI()
end)
